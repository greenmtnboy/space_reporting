import raw.satellite;


where base_category = 'P' and launch_date.year >= 2025
select
    jcat,
    name,
    type,
    #launch_date,
    launch_date,
    coalesce(decom_date, current_date()) as end_date,
    owner.e_name,
    owner.color,
    perigee,
    apogee,
    inc,
    launch.site.latitude,
    launch.site.longitude,
    date_diff(launch_date, decom_date, day)::int as mission_duration_days
order by   
    mission_duration_days asc limit 100;
