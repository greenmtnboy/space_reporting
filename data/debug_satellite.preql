import raw.satellite;


where base_category = 'P'   
and apogee < 1000000
and launch_date.year = 2025
and decom_date is not null
select
    jcat,
    name,
    #launch_date,
    launch_date,
    decom_date,
    owner.e_name,
    owner.color,
    perigee,
    apogee,
    inc,
    launch.site.latitude,
    launch.site.longitude
order by decom_date asc
limit 10;