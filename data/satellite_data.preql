import raw.satellite;

copy into json `gcs://trilogy_public_models/duckdb/launch_report/satellites_over_time/raw_data.json` from
where base_category = 'P'
select
    jcat,
    name,
    #launch_date,
    launch_date,
    coalesce(decom_date, current_date()) as end_date,
    owner.e_name,
    owner.color,
    perigee,
    apogee,
    inc,
    launch.site.latitude,
    launch.site.longitude
;